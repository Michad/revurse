{
    "compilerOptions": {
	"removeComments": false,
	"preserveConstEnums": true,
	"sourceMap": true,
	"declaration": true,
	"noImplicitAny": false,
	"noImplicitReturns": true,
	"esModuleInterop": true,
	"allowSyntheticDefaultImports": true,
	"strictNullChecks": true,
	"target": "es2015",
	"outDir": "dist",
	"module": "commonjs",
	"moduleResolution": "node",
	"baseUrl": "src",
    },
    "include": [
	"src"
    ],
    "filesGlob": [
	"src/"
    ]
}
